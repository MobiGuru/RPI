## Extra Packages
CONFIG_PACKAGE_autocore-x86=y
CONFIG_PACKAGE_automount=y

## Virtualization
# CONFIG_PACKAGE_kmod-irqbypass is not set 
# CONFIG_PACKAGE_kmod-kvm-amd is not set 
# CONFIG_PACKAGE_kmod-kvm-intel is not set 
# CONFIG_PACKAGE_kmod-kvm-x86 is not set 

## disable ipv6
## 如果加入 # CONFIG_IPV6 is not set 
## docker-ce 不会编译进固件
# CONFIG_KERNEL_IPV6 is not set
# CONFIG_PACKAGE_ipv6helper is not set

## TurboACC
## flowoffload
# CONFIG_PACKAGE_kmod-ipt-offload is not set
# CONFIG_PACKAGE_kmod-nf-flow is not set
# CONFIG_PACKAGE_luci-app-flowoffload is not set
## sfe
CONFIG_PACKAGE_kmod-fast-classifier=y
CONFIG_PACKAGE_kmod-shortcut-fe=y
CONFIG_PACKAGE_luci-app-sfe=y

## docker-ce
CONFIG_PACKAGE_docker-ce=y
CONFIG_DOCKER_KERNEL_OPTIONS=y
# CONFIG_DOCKER_SECCOMP is not set
# CONFIG_DOCKER_RES_SHAPE is not set
CONFIG_DOCKER_NET_OVERLAY=y
# CONFIG_DOCKER_NET_ENCRYPT is not set
CONFIG_DOCKER_NET_MACVLAN=y
# CONFIG_DOCKER_NET_TFTP is not set
CONFIG_DOCKER_STO_EXT4=y
CONFIG_DOCKER_STO_BTRFS=y

## package
# CONFIG_PACKAGE_nano is not set
# CONFIG_PACKAGE_strongswan is not set
CONFIG_PACKAGE_htop=y
CONFIG_PACKAGE_wget=y
CONFIG_PACKAGE_iperf3=y
CONFIG_PACKAGE_irqbalance=y
CONFIG_PACKAGE_unzip=y
CONFIG_PACKAGE_lm-sensors=y
# CONFIG_PACKAGE_lm-sensors-detect is not set 
CONFIG_PACKAGE_lscpu=y
CONFIG_PACKAGE_vim-fuller=y
CONFIG_PACKAGE_zsh=y

